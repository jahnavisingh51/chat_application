#include <stdio.h>

void selectionsort(int arr[], int n) {
    int i, j, min_idx, temp;
    for(i = 0; i < n - 1; i++) {
        min_idx = i;
        for(j = i + 1; j < n; j++) {
            if(arr[j] < arr[min_idx]) {
                min_idx = j;
            }
        }
        // Swap arr[min_idx] and arr[i]
        temp = arr[min_idx];
        arr[min_idx] = arr[i];
        arr[i] = temp;
    }
}

int main() {
    int n;
    scanf("%d", &n);

    int arr[n], even[n], odd[n];
    int e = 0, o = 0;

    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if(arr[i] % 2 == 0) {
            even[e++] = arr[i];
        } else {
            odd[o++] = arr[i];
        }
    }

    selectionsort(even, e);
    selectionsort(odd, o);

    // Print even numbers
    for(int i = 0; i < e; i++) {
        printf("%d ", even[i]);
    }
    printf("\n");

    // Print odd numbers
    for(int i = 0; i < o; i++) {
        printf("%d ", odd[i]);
    }
    printf("\n");

    return 0;
}
